      \strng{namehash}{4eebff4f498a6c504821e0abc89187b2}
      \strng{fullhash}{d771afd45f18fed21e5ba62bb6edf270}
      \strng{fullhashraw}{d771afd45f18fed21e5ba62bb6edf270}
      \strng{bibnamehash}{4eebff4f498a6c504821e0abc89187b2}
      \strng{authorbibnamehash}{4eebff4f498a6c504821e0abc89187b2}
